{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "last 1 version"
          ]
        },
        "modules": false,
        "useBuiltIns": false,
        "loose": true
      }
    ]
  ],
  "plugins": [
    "babel-plugin-dev-expression",
    "@babel/plugin-transform-modules-commonjs",
    "@babel/plugin-proposal-object-rest-spread"
  ]
}
